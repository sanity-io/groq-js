{
  "name": "groq-js",
  "version": "0.4.0-beta.2",
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "main": "./dist/groqjs.umd.cjs",
  "types": "./dist/groqjs.umd.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/groqjs.esm.mjs",
        "types": "./dist/groqjs.esm.d.ts"
      },
      "require": {
        "default": "./dist/groqjs.umd.cjs",
        "types": "./dist/groqjs.umd.d.ts"
      }
    },
    "./v2": {
      "import": {
        "default": "./dist/v2.esm.mjs",
        "types": "./dist/v2.esm.d.ts"
      },
      "require": {
        "default": "./dist/v2.umd.cjs",
        "types": "./dist/v2.umd.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rimraf dist; rollup -c",
    "test:generate": "./test/generate.sh",
    "test": "tap --no-timeout test/*.test.*",
    "prettify": "prettier --write src/**/*.ts src/*.ts",
    "prettify-check": "prettier --check src/**/*.ts src/*.ts"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "esbuild": "^0.14.2",
    "esbuild-register": "^3.2.0",
    "eslint-config-sanity": "^5.1.0",
    "ndjson": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "semver": "^7.3.5",
    "tap": "^15.0.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "tap": {
    "node-arg": [
      "-r",
      "esbuild-register"
    ],
    "check-coverage": false
  }
}

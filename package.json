  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/groq-js.esm.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test:jest": "tsdx test",
    "test:tap": "[ -e test/suite.taptest.js ] && tap --no-timeout test/suite.taptest.js",

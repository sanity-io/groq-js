{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "allowJs": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist"
  }
}
